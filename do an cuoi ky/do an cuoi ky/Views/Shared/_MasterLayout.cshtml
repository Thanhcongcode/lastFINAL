<body @*style=" background-image: url(https://bizweb.dktcdn.net/100/384/579/themes/839203/assets/main_bg.jpg?1662712768316);"*@>

    <div class="header-area">
        <div class="container">
            <div class="row">
                <div style="float:right">
                    <div class="user-menu">
                        <ul>
                            @if (kh != null)
                            {
                                <li>
                                    <p style="color : blanchedalmond  ; font-weight:bold; font-size:medium">
                                        @kh.TenDangNhap
                                    </p>
                                </li>
                                <li><p style="font-weight:bold; font-size:medium; color:blanchedalmond"> |</p></li>
                                <li><a style="color:blanchedalmond; font-weight:bold;font-size:medium" href="~/Users/Logout">Logout</a></li>
                            }
                        </ul>
                    </div>
                </div>


            </div>
        </div>
    </div> <!-- End header area -->
    <!--SEARCH-->

    <div class="site-branding-area">
        <div class="container">
            <div class="row">
                <div>
                    <div class="col-lg-4">
                        <div class="logo" style="float:left">
                            <h1><a href="./"><img src="~/Asset/img/logo2-removebg-preview.png"></a></h1>
                        </div>

                    </div>
                </div>
                <div class="col-lg-4" style="margin-top:40px">
                    <div class="wrap">
                        <form action="" class="search" method="post">
                            <input type="text" class="searchTerm" placeholder="Tìm kiếm..." style="border-radius: 5px 5px 5px 5px;background:black;color:blanchedalmond">
                            <button type="submit" class="searchButton" style="border-radius: 5px 5px 5px 5px;background:black">
                                <img style="width:40px;height:40px" src="~/Asset/img/kinh_lup-removebg-preview.png"/>
                            </button>
                        </form>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="shopping-item">
                        <div class="ml-auto">@Html.Action("GioHangPartial", "GioHang")</div>
                    </div>

                </div>


            </div>
        </div>

    </div> <!-- End site branding area --> 

    <div class="mainmenu-area">
        <div class="container">
            <div class="row">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li class="active">@Html.ActionLink("Trang Chủ", "Index", "Home")</li>
                        <li>@Html.ActionLink("Giới Thiệu", "Introduction", "Home")</li>
                        <li> @Html.ActionLink("Sản Phẩm", "Index", "MoHinh")</li>
                        @if (kh == null)
                        {
                            <li><a href="/Users/Login">Đăng nhập</a></li>
                        }
                        <li><a href="/Users/Dangky">Đăng ký</a></li>

                    </ul>
                </div>
            </div>
        </div>
    </div> <!-- End mainmenu area -->
    @RenderBody();
    <!-- End product widget area -->

